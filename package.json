{
	"name": "marklines",
	"displayName": "Lines for MARK Comments",
	"description": "Formats XCode-style \"MARK\" separator comments",
	"version": "0.1.1",
	"publisher": "jrpat",
	"license": "MIT",
	"engines": {
		"vscode": "^1.32.0"
	},
	"icon": ".github/media/icon.png",
	"galleryBanner": {
		"color": "#202026",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jrpat/vscode-marklines"
	},
	"bugs": {
		"type": "git",
		"url": "https://github.com/jrpat/vscode-marklines"
	},
